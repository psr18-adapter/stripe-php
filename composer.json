{
    "name": "psr18-adapter/stripe-php",
    "autoload": {
        "psr-4": {
            "Psr18Adapter\\Stripe\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Psr18Adapter\\Stripe\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "keywords": ["stripe", "psr-18"],
    "license": "MIT",
    "homepage": "https://github.com/psr18-adapter/stripe-php",
    "require": {
        "php": ">=7.1",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0",
        "stripe/stripe-php": ">=v2.1.3"
    },
    "require-dev": {
        "nyholm/psr7": "^1.3",
        "php-http/mock-client": "^1.4",
        "phpunit/phpunit": ">=5.7"
    }
}
